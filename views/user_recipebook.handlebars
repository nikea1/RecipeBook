<h1>User RecipeBooks</h1>

{{#each recipebook}}
	<a href="/recipebook/{{dataValues.id}}">
		<p>{{ dataValues.name }}</p>
	</a>
	{{#if dataValues.private}}
		<p>Private</p>
	{{/if}}
	{{#unless dataValues.private}}
		<p>Public</p>
	{{/unless}}
	{{#if dataValues.paid}}
		<p>Paid</p>
	{{/if}}
{{/each}}
<a href="/create_new_recipebook">Add new recipebook</a>